{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/master/docs/sources/developers/plugins/plugin.schema.json",
  "alerting": true,
  "dependencies": {
    "grafanaDependency": ">=8.3.0",
    "plugins": []
  },
  "id": "volkovlabs-rss-datasource",
  "info": {
    "author": {
      "name": "Volkov Labs",
      "url": "https://volkovlabs.io"
    },
    "description": "RSS/Atom datasource plugin for Grafana",
    "keywords": ["rss", "atom", "news"],
    "links": [
      {
        "name": "Website",
        "url": "https://volkovlabs.io"
      }
    ],
    "logos": {
      "large": "img/logo.svg",
      "small": "img/logo.svg"
    },
    "screenshots": [
      {
        "name": "Dashboard",
        "path": "img/dashboard.png"
      },
      {
        "name": "Panel",
        "path": "img/panel.png"
      }
    ],
    "updated": "%TODAY%",
    "version": "%VERSION%"
  },
  "metrics": true,
  "name": "RSS/Atom",
  "routes": [
    {
      "method": "GET",
      "path": "feed",
      "reqRole": "Viewer",
      "url": "{{ .JsonData.feed }}"
    }
  ],
  "type": "datasource"
}
