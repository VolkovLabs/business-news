{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/master/docs/sources/developers/plugins/plugin.schema.json",
  "alerting": false,
  "dependencies": {
    "grafanaDependency": ">=10.1.0",
    "plugins": []
  },
  "id": "volkovlabs-rss-datasource",
  "info": {
    "author": {
      "name": "Volkov Labs",
      "url": "https://volkovlabs.io"
    },
    "description": "Visualize RSS/Atom feeds",
    "keywords": ["rss", "atom", "news"],
    "links": [
      {
        "name": "Documentation",
        "url": "https://volkovlabs.io/plugins/volkovlabs-rss-datasource/"
      },
      {
        "name": "GitHub",
        "url": "https://github.com/VolkovLabs/business-news"
      }
    ],
    "logos": {
      "large": "img/logo.svg",
      "small": "img/logo.svg"
    },
    "screenshots": [
      {
        "name": "Dashboard",
        "path": "img/dashboard.png"
      }
    ],
    "updated": "%TODAY%",
    "version": "%VERSION%"
  },
  "metrics": true,
  "name": "Business News",
  "routes": [
    {
      "method": "*",
      "path": "feed",
      "reqRole": "Viewer",
      "url": "{{ .JsonData.feed }}"
    }
  ],
  "type": "datasource"
}
